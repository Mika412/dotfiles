* {
    background-color:           #282a36;
    border-color:               #282c34;
    text-color:                 #f8f8f2;
    /* font-weight:                bold; */
    font:                       "FiraMono Nerd Font Mono Bold 13";
    /* font:                       "Ubuntu Bold 9"; */
    prompt-background:          #51afef;
    prompt-foreground:          #282c34;
    prompt-padding:             4px;
    selected-normal-background: #50fa7b;
    selected-normal-foreground: #44475a;
    /* selected-normal-foreground: #ffffff; */
    highlight:     underline bold #ffffff;
}
